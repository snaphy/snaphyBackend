{
    "name": "Cuisines",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "name": {
            "type": "string",
            "required": true,
            "template": {
                "type": "input",
                "templateOptions": {
                    "type": "text",
                    "label": "Enter cuisine name",
                    "id": "cuisinesName"
                }
            }
        }
    },
    "validations": [],
    "relations": {
        "recipes": {
            "type": "hasAndBelongsToMany",
            "model": "Recipe",
            "foreignKey": ""
        }
    },
    "validationsBackend": {
      "rules": {
        "name": {
          "required": true,
          "minlength": "3"
        }
      },
      "messages": {
        "name": {
          "required": "cuisines name is required",
          "minlength": "Minimum permitted length is atleast 3"
        }
      }
    },
    "acls": [],
    "methods": []
}
