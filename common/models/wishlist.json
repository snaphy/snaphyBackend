{
  "name": "Wishlist",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "validationsBackend": {
    "rules": {
      "customer":{
        "required": true,
        "valueNotEquals": ""
      }
    },
    "messages": {
      "customer":{
        "required": "Please add chef of this recipe",
        "valueNotEquals": "Please add chef of this recipe"
      }
    }
  },
  "tables": {
    "customer_phoneNumber": {
      "display": false
    },
    "customer_firstName": {
      "display": false
    },
    "customer_lastName": {
      "display": false
    },
    "customer_profilePic": {
      "display": false
    },
    "customer_password": {
      "display": false
    },
    "customer_date": {
      "display": false
    },
    "customer_email": {
      "display": true,
      "search": "related",
      "relatedModel": "Customer",
      "foreignKey": "customerId",
      "propertyName": "email"
    }
  },
  "properties": {
    "id": {
      "type": {
        "template": {
          "type": "dummy"
        }
      }
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Customer",
        "searchProp": "email",
        "create": false,
        "id": "customerIdName"
      }
    },
    "recipes": {
      "type": "hasAndBelongsToMany",
      "model": "Recipe",
      "foreignKey": "",
      "templateOptions": {
        "btnText": "Add Recipe",
        "searchProp": "name",
        "create": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
